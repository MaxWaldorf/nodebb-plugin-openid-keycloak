{
	"id": "nodebb-plugin-sso-keycloak",
	"name": "NodeBB Wordpress SSO",
	"description": "Allows users to log in via a configured Wordpress account",
	"url": "https://github.com/MaxWaldorf/nodebb-plugin-sso-keycloak",
	"library": "./library.js",
	"hooks": [
		{ "hook": "static:app.preload", "method": "addMiddleware" },
		{ "hook": "static:app.load", "method": "init" },
		{ "hook": "filter:admin.header.build", "method": "addAdminNavigation" },
		{ "hook": "static:user.delete", "method": "deleteUserData" },
		{ "hook": "filter:auth.init", "method": "getStrategy" }
	],
	"modules": {
		"../admin/plugins/sso-keycloak.js": "./static/lib/admin.js"
	},
	"templates": "static/templates"
}
